# Get your PostgreSQL database from Neon (https://neon.tech) 

POSTGRES_URL=postgresql://username:password@host/database?sslmode=require

# EmailJS Configuration
# Sign up at https://www.emailjs.com to get these credentials
EMAILJS_PRIVATE_KEY=your_emailjs_private_key
EMAILJS_SERVICE_ID=service_xxxxxxx
EMAILJS_TEMPLATE_ID=template_xxxxxxx
EMAILJS_USER_ID=your_emailjs_user_id

# Security Tokens
# Generate a secure random token for API authentication
SECRET_TOKEN=your_secret_token_here

# Cron job authentication (for Vercel scheduled functions)
CRON_SECRET=your_cron_secret_here

TO_EMAIL=add_your_to_email
LEETCODE_USERNAME=add_your_leetcode_username

# Analytics flag (1 to enable, 0 to disable)
NEXT_PUBLIC_ANALYTICS_FLAG=1


KV_URL=vercel_redis_url_here
KV_REST_API_URL=vercel_redis_url_here
KV_REST_API_TOKEN=vercel_redis_secret_here
KV_REST_API_READ_ONLY_TOKEN=vercel_redis_secret_here
REDIS_URL=vercel_redis_url_here